# EDINET API Configuration
EDINET_API_KEY=your_edinet_api_key_here

# --- LLM Configuration ---
# Primary LLM model configuration
LLM_MODEL=gpt-4o
LLM_FALLBACK_MODEL=gpt-4-turbo

# OpenAI Configuration (most common)
OPENAI_API_KEY=your_openai_api_key_here

# Alternative: Generic LLM key (can override OPENAI_API_KEY)
# LLM_API_KEY=your_llm_api_key_here

# --- Azure OpenAI Configuration (Optional) ---
# Uncomment and configure if using Azure OpenAI
# AZURE_OPENAI_API_KEY=your_azure_openai_api_key
# AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
# AZURE_OPENAI_API_VERSION=2023-07-01-preview
# AZURE_OPENAI_DEPLOYMENT=your_deployment_name

# --- Other LLM Provider Examples ---
# For Anthropic Claude (requires llm-anthropic plugin)
# ANTHROPIC_API_KEY=your_anthropic_api_key

# For Google Gemini (requires llm-gemini plugin)
# GEMINI_API_KEY=your_gemini_api_key
